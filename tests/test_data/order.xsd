<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.sterlingcommerce.com/documentation"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:yfc="http://www.sterlingcommerce.com/documentation"
    xmlns:yfctype="http://www.sterlingcommerce.com/documentation/types">
    <xsd:annotation yfc:fileName="OrderEventToAzure.xsd" yfc:rootElement="Event">
        <xsd:appinfo source="yfc:structure">
            <Event>
                <EventDetails hasAttributes="Y" />
                <CurrentState>
                    <Order hasAttributes="Y">
                        <Shipments>
                            <Shipment hasAttributes="Y">
                                <Extn hasAttributes="Y" />
                            </Shipment>
                        </Shipments>
                        <OrderLines>
                            <OrderLine hasAttributes="Y">
                                <Instructions hasAttributes="Y">
                                    <Instruction hasAttributes="Y" />
                                </Instructions>
                                <Item hasAttributes="Y" />
                                <LineInvoicedTotals hasAttributes="Y" />
                                <OrderStatuses>
                                    <OrderStatus hasAttributes="Y">
                                        <Details hasAttributes="Y" />
                                        <OrderStatusTranQuantity hasAttributes="Y" />
                                    </OrderStatus>
                                </OrderStatuses>
                                <ItemDetails hasAttributes="Y">
                                    <PrimaryInformation hasAttributes="Y" />
                                    <ClassificationCodes hasAttributes="Y" />
                                </ItemDetails>
                                <PersonInfoShipTo hasAttributes="Y">
                                    <Extn hasAttributes="Y" />
                                </PersonInfoShipTo>
                                <LineRemainingTotals hasAttributes="Y" />
                                <Schedules>
                                    <Schedule hasAttributes="Y" />
                                </Schedules>
                                <LineCharges>
                                    <LineCharge hasAttributes="Y">
                                        <ChargeCategoryDetails hasAttributes="Y" />
                                        <ChargeNameDetails hasAttributes="Y" />
                                    </LineCharge>
                                </LineCharges>
                                <LineOverallTotals hasAttributes="Y" />
                                <ShipmentLines>
                                    <ShipmentLine hasAttributes="Y">
                                        <ContainerDetails>
                                            <ContainerDetail hasAttributes="Y" />
                                        </ContainerDetails>
                                    </ShipmentLine>
                                </ShipmentLines>
                                <Extn hasAttributes="Y">
                                    <MarkDownDetailList>
                                        <MarkDownDetail hasAttributes="Y" />
                                    </MarkDownDetailList>
                                </Extn>
                                <LinePriceInfo hasAttributes="Y" />
                                <BundleParentLine hasAttributes="Y" />
                                <LineTaxes>
                                    <LineTax hasAttributes="Y" />
                                    <TaxSummary>
                                        <TaxSummaryDetail hasAttributes="Y" />
                                    </TaxSummary>
                                </LineTaxes>
                                <KitLines hasAttributes="Y" />
                                <References>
                                    <Reference hasAttributes="Y" />
                                </References>
                                <OrderLineTranQuantity hasAttributes="Y" />
                                <Notes hasAttributes="Y">
                                    <Note hasAttributes="Y">
                                        <User hasAttributes="Y" />
                                    </Note>
                                </Notes>
                                <AdditionalAddresses hasAttributes="Y" />
                                <DerivedFromOrderLine hasAttributes="Y">
                                    <Extn hasAttributes="Y" />
                                    <ItemDetails hasAttributes="Y">
                                        <PrimaryInformation hasAttributes="Y" />
                                    </ItemDetails>
                                    <Item hasAttributes="Y" />
                                </DerivedFromOrderLine>
                                <DerivedFromOrder hasAttributes="Y">
                                    <Extn hasAttributes="Y" />
                                </DerivedFromOrder>
                            </OrderLine>
                        </OrderLines>
                        <PersonInfoBillTo hasAttributes="Y">
                            <Extn hasAttributes="Y" />
                        </PersonInfoBillTo>
                        <PersonInfoShipTo hasAttributes="Y">
                            <Extn hasAttributes="Y" />
                        </PersonInfoShipTo>
                        <InvoicedTotals hasAttributes="Y" />
                        <PaymentMethods>
                            <PaymentMethod hasAttributes="Y">
                                <Extn hasAttributes="Y" />
                            </PaymentMethod>
                        </PaymentMethods>
                        <ChargeTransactionDetails hasAttributes="Y">
                            <ChargeTransactionDetail hasAttributes="Y">
                                <CreditCardTransactions>
                                    <CreditCardTransaction hasAttributes="Y" />
                                </CreditCardTransactions>
                                <PaymentMethod hasAttributes="Y" />
                            </ChargeTransactionDetail>
                        </ChargeTransactionDetails>
                        <ReturnOrdersForExchange>
                            <ReturnOrderForExchange hasAttributes="Y">
                                <Extn hasAttributes="Y" />
                            </ReturnOrderForExchange>
                        </ReturnOrdersForExchange>
                        <Customer hasAttributes="Y">
                            <CustomerContactList>
                                <CustomerContact hasAttributes="Y">
                                    <CustomerAdditionalAddressList>
                                        <CustomerAdditionalAddress hasAttributes="Y">
                                            <PersonInfo hasAttributes="Y">
                                                <Extn hasAttributes="Y" />
                                            </PersonInfo>
                                        </CustomerAdditionalAddress>
                                    </CustomerAdditionalAddressList>
                                </CustomerContact>
                            </CustomerContactList>
                            <Extn hasAttributes="Y" />
                        </Customer>
                        <RemainingTotals hasAttributes="Y" />
                        <Notes hasAttributes="Y">
                            <Note hasAttributes="Y" />
                        </Notes>
                        <Instructions hasAttributes="Y" />
                        <Extn hasAttributes="Y" />
                        <OverallTotals hasAttributes="Y" />
                        <OrderHoldTypes>
                            <OrderHoldType hasAttributes="Y" />
                        </OrderHoldTypes>
                        <PriceInfo hasAttributes="Y" />
                        <HeaderCharges>
                            <HeaderCharge hasAttributes="Y" />
                        </HeaderCharges>
                        <AdditionalAddresses hasAttributes="Y" />
                        <ExchangeOrders>
                            <ExchangeOrder hasAttributes="Y">
                                <Extn hasAttributes="Y" />
                            </ExchangeOrder>
                        </ExchangeOrders>
                        <OrderAudits>
                            <OrderAudit hasAttributes="Y">
                                <Order hasAttributes="Y" />
                                <OrderAuditLevels>
                                    <OrderAuditLevel hasAttributes="Y">
                                        <ModificationTypes>
                                            <ModificationType hasAttributes="Y" />
                                        </ModificationTypes>
                                        <OrderAuditDetails>
                                            <OrderAuditDetail hasAttributes="Y">
                                                <Attributes>
                                                    <Attribute hasAttributes="Y" />
                                                </Attributes>
                                            </OrderAuditDetail>
                                        </OrderAuditDetails>
                                        <OrderLine hasAttributes="Y" />
                                    </OrderAuditLevel>
                                </OrderAuditLevels>
                            </OrderAudit>
                        </OrderAudits>
                    </Order>
                </CurrentState>
                <StateChanges />
            </Event>
        </xsd:appinfo>
    </xsd:annotation>
    <xsd:import
        namespace="http://www.sterlingcommerce.com/documentation/types"
        schemaLocation="datatypes.xsd" />
    <xsd:element name="Event" type="yfc:EventXSDType">
        <xsd:annotation yfc:tableName="YFS_EVENT">
            <xsd:documentation>This table lists the various events raised by the transactions.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="EventXSDType">
        <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="1" ref="yfc:EventDetails" />
            <xsd:element maxOccurs="1" minOccurs="1" ref="yfc:CurrentState" />
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:StateChanges" />
        </xsd:all>
    </xsd:complexType>
    <xsd:element name="EventDetails">
        <xsd:complexType>
            <xsd:attribute name="EntitySubType" type="xsd:string" use="optional" />
            <xsd:attribute name="EntityType" type="xsd:string" use="optional" />
            <xsd:attribute name="EntityVersion" type="xsd:string" use="optional" />
            <xsd:attribute name="EventData" type="xsd:string" use="optional" />
            <xsd:attribute name="EventSource" type="xsd:string" use="optional" />
            <xsd:attribute name="EventSubType" type="xsd:string" use="optional" />
            <xsd:attribute name="EventTarget" type="xsd:string" use="optional" />
            <xsd:attribute name="EventTime" type="xsd:string" use="optional" />
            <xsd:attribute name="EventType" type="xsd:string" use="optional" />
            <xsd:attribute name="MasterOrderNumber" type="xsd:string" use="optional" />
            <xsd:attribute name="OrderAncestry" type="xsd:string" use="optional" />
            <xsd:attribute name="OrderChannel" type="xsd:string" use="optional" />
            <xsd:attribute name="OrderEnterprise" type="xsd:string" use="optional" />
            <xsd:attribute name="OrderNumber" type="xsd:string" use="optional" />
            <xsd:attribute name="OrderProcessType" type="xsd:string" use="optional" />
            <xsd:attribute name="OrderSource" type="xsd:string" use="optional" />
            <xsd:attribute name="OrderType" type="xsd:string" use="optional" />
            <xsd:attribute name="ParentOrderNumber" type="xsd:string" use="optional" />
            <xsd:attribute name="ResentFlag" type="xsd:string" use="optional" />
            <xsd:attribute name="ResentTime" type="xsd:string" use="optional" />
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="CurrentState">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="1" name="Order">
                    <xsd:annotation yfc:tableName="YFS_ORDER_HEADER">
                        <xsd:documentation>This table stores all order documents in the system.
                            These orders can be further classified into various types of documents.
                            For example, ORDERS, PLANNED ORDERS and any other future types that the
                            system may provide.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Shipments" />
                            <xsd:element maxOccurs="1" minOccurs="1" ref="yfc:OrderLines" />
                            <xsd:element maxOccurs="1" minOccurs="1" ref="yfc:PersonInfoBillTo" />
                            <xsd:element maxOccurs="1" minOccurs="1" name="PersonInfoShipTo">
                                <xsd:annotation yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>This table stores relevant personal
                                        information and addresses. Addresses are typically
                                        organization address, ship node address, customer address,
                                        and so forth. Personal information is required in cases
                                        where person to person contact needs to be established; for
                                        example, taking appointments or speaking during
                                        negotiations. The key of this table is posted in various
                                        tables to allow access to people and address information.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:all>
                                        <xsd:element maxOccurs="1"
                                            minOccurs="0" name="Extn">
                                            <xsd:complexType>
                                                <xsd:attribute
                                                    name="IRLAddressLine1"
                                                    type="xsd:string" use="optional" />
                                                <xsd:attribute
                                                    name="IRLAddressLine2"
                                                    type="xsd:string" use="optional" />
                                                <xsd:attribute
                                                    name="IRLAddressLine3"
                                                    type="xsd:string" use="optional" />
                                                <xsd:attribute
                                                    name="IRLAddressLine4"
                                                    type="xsd:string" use="optional" />
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:all>
                                    <xsd:attribute name="AddressLine1"
                                        type="yfctype:Address" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="ADDRESS_LINE1"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The address line 1 field.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AddressLine2"
                                        type="yfctype:Address" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="ADDRESS_LINE2"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The address line 2 field.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AddressLine3"
                                        type="yfctype:Address" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="ADDRESS_LINE3"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The address line 3 field.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AddressLine4"
                                        type="yfctype:Address" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="ADDRESS_LINE4"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The address line 4 field.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AddressLine5"
                                        type="yfctype:Address" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="ADDRESS_LINE5"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The address line 5 field.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AddressLine6"
                                        type="yfctype:Address" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="ADDRESS_LINE6"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The address line 6 field.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                        name="AlternateEmailID"
                                        type="yfctype:EMailID" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="ALTERNATE_EMAILID"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The alternate email address at which
                                                the person or a predesignated employee of the
                                                organization referred to in the address can be
                                                contacted.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Beeper"
                                        type="yfctype:PhoneNo" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="BEEPER" yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The beeper number at which the person
                                                or a predesignated employee of the organization
                                                referred to in the address can be called.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="City"
                                        type="yfctype:City" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="CITY" yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The city in the address.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Company"
                                        type="yfctype:Company" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="COMPANY" yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The company this person is associated
                                                with.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Country"
                                        type="yfctype:Country" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="COUNTRY" yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The country in the address.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Createprogid"
                                        type="yfctype:ProgramID" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="CREATEPROGID"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The program that created this record.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Createts"
                                        type="yfctype:TimeStamp" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="CREATETS"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The timestamp for the creation of
                                                this record.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Createuserid"
                                        type="yfctype:Createuserid" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="CREATEUSERID"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The user who created this record.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DayFaxNo"
                                        type="yfctype:PhoneNo" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="DAY_FAX_NO"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The day time fax at which the person
                                                or a predesignated employee of the organization
                                                referred to in the address can be notified.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DayPhone"
                                        type="yfctype:PhoneNo" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="DAY_PHONE"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The day time phone at which the
                                                person or organization referred to in the address
                                                can be called.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Department"
                                        type="yfctype:Department" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="DEPARTMENT"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The primary department in charge of
                                                procurement.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="EMailID"
                                        type="yfctype:EMailID" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="EMAILID" yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The email address at which the person
                                                or a predesignated employee of the organization
                                                referred to in the address can be contacted.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ErrorTxt"
                                        type="yfctype:ErrorText" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="ERROR_TXT"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>This field is currently not used.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="EveningFaxNo"
                                        type="yfctype:PhoneNo" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="EVENING_FAX_NO"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The evening time fax at which the
                                                person or a predesignated employee of the
                                                organization referred to in the address can be
                                                notified.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="EveningPhone"
                                        type="yfctype:PhoneNo" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="EVENING_PHONE"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The evening time phone at which the
                                                person or organization referred to in the address
                                                can be called.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="FirstName"
                                        type="yfctype:PersonName" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="FIRST_NAME"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The first name of the person.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="HttpUrl"
                                        type="yfctype:URL" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="HTTP_URL"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The URL of the person or organization
                                                referred to in this contact information.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                        name="IsAddressVerified"
                                        type="yfctype:Flag" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="IS_ADDRESS_VERIFIED"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>This flag indicates whether or not
                                                the address in this record has been verified by an
                                                address verification program. Intended for future
                                                use.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                        name="IsCommercialAddress"
                                        type="yfctype:Flag" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="IS_COMMERCIAL_ADDRESS"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>Indicates whether the address is
                                                commercial, residential or none. Values of this
                                                field can be Y -Commercial address.N - Residential
                                                address. undefined for Null.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="JobTitle"
                                        type="yfctype:Title-40" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="JOB_TITLE"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The job title of the person.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="LastName"
                                        type="yfctype:PersonName" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="LAST_NAME"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The last name of the person.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Latitude" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="LATITUDE"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The latitude of this location.</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:simpleType>
                                            <xsd:union>
                                                <xsd:simpleType>
                                                    <xsd:restriction
                                                        base="yfctype:DecimalWithNegative" />
                                                </xsd:simpleType>
                                                <xsd:simpleType>
                                                    <xsd:restriction base="xsd:string">
                                                        <xsd:enumeration value="" />
                                                    </xsd:restriction>
                                                </xsd:simpleType>
                                            </xsd:union>
                                        </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute name="Lockid"
                                        type="yfctype:Lockid" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="LOCKID" yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>An integer used to track and prevent
                                                concurrent modifications.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Longitude" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="LONGITUDE"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The longitude of this location.</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:simpleType>
                                            <xsd:union>
                                                <xsd:simpleType>
                                                    <xsd:restriction
                                                        base="yfctype:DecimalWithNegative" />
                                                </xsd:simpleType>
                                                <xsd:simpleType>
                                                    <xsd:restriction base="xsd:string">
                                                        <xsd:enumeration value="" />
                                                    </xsd:restriction>
                                                </xsd:simpleType>
                                            </xsd:union>
                                        </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute name="MiddleName"
                                        type="yfctype:MiddleName" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="MIDDLE_NAME"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The middle name of the person.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="MobilePhone"
                                        type="yfctype:PhoneNo" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="MOBILE_PHONE"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The mobile phone at which the person
                                                or a predesignated employee of the organization
                                                referred to in the address can be called.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Modifyprogid"
                                        type="yfctype:ProgramID" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="MODIFYPROGID"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The program that last modified this
                                                record.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Modifyts"
                                        type="yfctype:TimeStamp" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="MODIFYTS"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The timestamp for the last
                                                modification of this record.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Modifyuserid"
                                        type="yfctype:Modifyuserid" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="MODIFYUSERID"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The user who last modified this
                                                record.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="OtherPhone"
                                        type="yfctype:PhoneNo" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="OTHER_PHONE"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The alternate phone number at which
                                                the person or a predesignated employee of the
                                                organization referred to in the address can be
                                                called.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PersonID"
                                        type="yfctype:PersonId" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="PERSON_ID"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The unique identifier for a person.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PersonInfoKey"
                                        type="yfctype:Key" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="PERSON_INFO_KEY"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The primary key for the
                                                YFS_PERSON_INFO table.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                        name="PreferredShipAddress"
                                        type="yfctype:Flag" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="PREFERRED_SHIP_ADDRESS"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The preferred ship address over and
                                                above the address in this record. Pointer to another
                                                record of this type in this table.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="State"
                                        type="yfctype:State" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="STATE" yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The state in the address.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Suffix"
                                        type="yfctype:Suffix" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="SUFFIX" yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The suffix for a person, such as Jr.,
                                                or Sr.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Title"
                                        type="yfctype:Title-10" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="TITLE" yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The title for the person such as Ms.,
                                                Mr., or Dr..</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="UseCount"
                                        type="yfctype:Count" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="USE_COUNT"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>This field is currently not used.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                        name="VerificationStatus"
                                        type="yfctype:ShortStatus" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="VERIFICATION_STATUS"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>This field is currently not used.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ZipCode"
                                        type="yfctype:ZipCode" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="ZIP_CODE"
                                            yfc:tableName="YFS_PERSON_INFO">
                                            <xsd:documentation>The zip code in the address.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="isHistory"
                                        type="xsd:string" use="optional" />
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:InvoicedTotals" />
                            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PaymentMethods" />
                            <xsd:element maxOccurs="1" minOccurs="0"
                                ref="yfc:ChargeTransactionDetails" />
                            <xsd:element maxOccurs="1" minOccurs="0"
                                ref="yfc:ReturnOrdersForExchange" />
                            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Customer" />
                            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:RemainingTotals" />
                            <xsd:element maxOccurs="1" minOccurs="0" name="Notes">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element
                                            maxOccurs="unbounded"
                                            minOccurs="1" name="Note">
                                            <xsd:annotation yfc:tableName="YFS_ORDER_HEADER">
                                                <xsd:documentation>This table stores all order
                                                    documents in the system. These orders can be
                                                    further classified into various types of
                                                    documents. For example, ORDERS, PLANNED ORDERS
                                                    and any other future types that the system may
                                                    provide.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:attribute
                                                    name="NoteText"
                                                    type="xsd:string" use="optional" />
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                    <xsd:attribute name="NumberOfNotes"
                                        type="xsd:string" use="optional" />
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element maxOccurs="1" minOccurs="0" name="Instructions">
                                <xsd:complexType>
                                    <xsd:attribute
                                        name="NumberOfInstructions"
                                        type="xsd:string" use="optional" />
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element maxOccurs="1" minOccurs="0" name="Extn">
                                <xsd:complexType>
                                    <xsd:attribute name="CashierID"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute name="CashierName"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute
                                        name="CommentsFromStore"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute
                                        name="ExtnChannelOrderNo"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute name="ExtnDty"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute name="ExtnEmpID"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute
                                        name="ExtnEnableOrderPurge"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute name="ExtnGSTNo"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute name="ExtnIRNAckNo"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute name="ExtnIRNNo"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute name="ExtnIRNStatus"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute
                                        name="ExtnInterSoreDocNo"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute
                                        name="ExtnManualOrderReadyForConfirm"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute
                                        name="ExtnRetnInvoiceOrderNo"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute
                                        name="ExtnSalesInvoiceOrderDate"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute
                                        name="ExtnSalesInvoiceOrderNo"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute
                                        name="ExtnSalesOrderNo"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute
                                        name="GiftWrappingFlag"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute
                                        name="MaxRepaymentDate"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute name="RejectedReason"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute name="SaleDate"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute name="TillNo"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute name="ZECPReason"
                                        type="xsd:string" use="optional" />
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OverallTotals" />
                            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderHoldTypes" />
                            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PriceInfo" />
                            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:HeaderCharges" />
                            <xsd:element maxOccurs="1" minOccurs="0" name="AdditionalAddresses">
                                <xsd:complexType>
                                    <xsd:attribute
                                        name="NumberOfAdditionalAddresses"
                                        type="xsd:string" use="optional" />
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ExchangeOrders" />
                            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderAudits" />
                        </xsd:all>
                        <xsd:attribute name="ActualPricingDate"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="AdjustmentInvoicePending"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="ADJUSTMENT_INVOICE_PENDING"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This flag indicates whether an adjustment invoice
                                    has to be generated.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AllAddressesVerified"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="ALL_ADDRESSES_VERIFIED"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This flag determines whether or not the addresses
                                    in this order have been verified by an address verification
                                    program.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ApprovalCycle"
                            type="yfctype:OrderApprovalCycle" use="optional">
                            <xsd:annotation
                                yfc:columnName="APPROVAL_CYCLE" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The approval cycle the order is currently in.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute
                            name="AuthorizationExpirationDate"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation
                                yfc:columnName="AUTHORIZATION_EXPIRATION_DATE"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The smallest expiration date of currently valid
                                    expirations. This field is used by the system payment processing
                                    agents. Any transaction having a financial implication will set
                                    this date to current system date. This field is used as a
                                    trigger by payment processing agents to create charge and
                                    authorization reqeusts.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="BillToID"
                            type="yfctype:ID-40" use="optional">
                            <xsd:annotation yfc:columnName="BILL_TO_ID"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The identifier of the bill to customer. If a
                                    customer definition representing the buyer organization exists
                                    within the system, the BillToID can represent the CustomerID of
                                    the customer. Otherwise, the BillToID can represent the PersonID
                                    of the bill to address or the BuyerOrganizationCode of the buyer
                                    organization.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="BillToKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation yfc:columnName="BILL_TO_KEY"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The bill to address that is associated with this
                                    order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CarrierAccountNo"
                            type="yfctype:AccountNo" use="optional">
                            <xsd:annotation
                                yfc:columnName="CUSTCARRIER_ACCOUNT_NO"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The account number of the customer with the
                                    carrier.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CarrierServiceCode"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation
                                yfc:columnName="CARRIER_SERVICE_CODE"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The carrier service code for this order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ChainType"
                            type="yfctype:EnumChainType" use="optional">
                            <xsd:annotation yfc:columnName="CHAIN_TYPE"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>Specifies the chaining type of the order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ChargeActualFreightFlag"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="CHARGE_ACTUAL_FREIGHT"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This flag indicates whether the actual freight
                                    charges should be passed on to the customer. If set to 'Y',
                                    invoicing picks up charges from shipments and uses that to
                                    invoice the customer.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ComplimentaryGiftBoxQty" use="optional">
                            <xsd:annotation
                                yfc:columnName="COMPL_GIFT_BOX_QTY" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>No. of complimentary gift boxes to be given out
                                    with the order.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                        <xsd:restriction base="yfctype:DelayWindow" />
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                        <xsd:restriction base="xsd:string">
                                            <xsd:enumeration value="" />
                                        </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="CreatedAtNode"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="CREATED_AT_NODE" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This flag indicates whether the order was created
                                    by a node. This field is used during blind return creation at
                                    the nodes.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Createprogid"
                            type="yfctype:ProgramID" use="optional">
                            <xsd:annotation
                                yfc:columnName="CREATEPROGID" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>Program that created the record.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Createts"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation yfc:columnName="CREATETS"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>Timestamp when the record was created.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Createuserid"
                            type="yfctype:Createuserid" use="optional">
                            <xsd:annotation
                                yfc:columnName="CREATEUSERID" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>User who created the record.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CustCustPONo"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="CUSTOMER_CUSTOMER_PO_NO"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The PO number of the customer's customer. In some
                                    situations, this may need to be printed on the actual packaging.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CustomerAge" use="optional">
                            <xsd:annotation
                                yfc:columnName="CUSTOMER_AGE" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This field is used to store age of the customer
                                    for the sale of item which requires age verification.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                        <xsd:restriction base="yfctype:Decimal" />
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                        <xsd:restriction base="xsd:string">
                                            <xsd:enumeration value="" />
                                        </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="CustomerContactID"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation
                                yfc:columnName="CUSTOMER_CONTACT_ID"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This is the customer contact ID for the order.
                                    This field maps to contact ID field of YFS_Customer_Contact.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CustomerEMailID"
                            type="yfctype:EMailID" use="optional">
                            <xsd:annotation
                                yfc:columnName="CUSTOMER_EMAILID" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The E-mail ID of the customer.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CustomerFirstName"
                            type="yfctype:Text-64" use="optional">
                            <xsd:annotation
                                yfc:columnName="CUSTOMER_FIRST_NAME"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>First name of the customer placing the order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CustomerLastName"
                            type="yfctype:Text-64" use="optional">
                            <xsd:annotation
                                yfc:columnName="CUSTOMER_LAST_NAME" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>Last name of the customer placing the order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CustomerPONo"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="CUSTOMER_PO_NO" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The PO number of the customer. For purchase
                                    orders, this can be used for storing the purchase order number
                                    of the purchasing system if that number is not already stored as
                                    part of the order number.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CustomerPhoneNo"
                            type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation
                                yfc:columnName="CUSTOMER_PHONE_NO" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>Daytime Phone number of the customer placing the
                                    order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CustomerZipCode"
                            type="yfctype:ZipCode" use="optional">
                            <xsd:annotation
                                yfc:columnName="CUSTOMER_ZIP_CODE" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>Address Zip Code of the customer placing the
                                    order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DeliveryCode"
                            type="yfctype:DeliveryCode" use="optional">
                            <xsd:annotation
                                yfc:columnName="DELIVERY_CODE" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The organization that pays the freight charges.
                                    For example, SUPPLIER, ENTERPRISE, and MARKETPLACE. However, you
                                    could pass any other value which is a valid shipment charge type
                                    as understood by the carrier by which the shipment is
                                    transported. See YFS_SHIPMENT_CONTAINER.carrier_payment_type for
                                    additional information.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Division"
                            type="yfctype:Division" use="optional">
                            <xsd:annotation yfc:columnName="DIVISION"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The order classification attribute. This field
                                    can be used for reporting purposes. Sterling Selling And
                                    Fulfillment Suite system has no logic based on this field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DoNotConsolidate"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="DO_NOT_CONSOLIDATE" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This flag indicates whether shipment constraints
                                    and routing based constraints should be used during release
                                    creation.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DocumentType"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation
                                yfc:columnName="DOCUMENT_TYPE" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The document type that is associated with this
                                    order. For example, Sales Order document (0001), Planned Order
                                    document (0002), Return Order document (0003) , and Purchase
                                    Order document (0005). This field determines the order's
                                    fulfillment cycle and controls the inventory and payment updates
                                    performed on the order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DraftOrderFlag"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="DRAFT_ORDER_FLAG" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This flag indicates whether the order is yet to
                                    be confirmed. Order lines can only be deleted from the system in
                                    draft stage. This flag is not applicable for Quotes.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="EnteredBy"
                            type="yfctype:Text-254" use="optional">
                            <xsd:annotation yfc:columnName="ENTERED_BY"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The system or sales person who created the order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="EnterpriseCode"
                            type="yfctype:OrgCode" use="optional">
                            <xsd:annotation
                                yfc:columnName="ENTERPRISE_KEY" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The enterprise that is associated with this
                                    order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="EntryType"
                            type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="ENTRY_TYPE"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The channel through which this order was created.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="FreightTerms"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation yfc:columnName="FOB" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>Indicates Freight Terms for the order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="HasDerivedChild"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="HAS_DERIVED_CHILD" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This flag indicates whether the order has any
                                    derived children.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="HasDerivedParent"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="HAS_DERIVED_PARENT" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This flag indicates whether the order has any
                                    derived parent.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="HoldFlag"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation yfc:columnName="HOLD_FLAG"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This flag indicates whether the order or any
                                    order line is on hold. If the order is on hold, default
                                    configuration will stop the scheduling and release process for
                                    the order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="HoldReasonCode"
                            type="yfctype:HoldReasonCode" use="optional">
                            <xsd:annotation
                                yfc:columnName="HOLD_REASON_CODE" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The reason code for putting the order on hold.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="InternalApp"
                            type="yfctype:Flag" use="optional" />
                        <xsd:attribute name="InvoiceComplete"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="INVOICE_COMPLETE" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This flag indicates whether invoicing is done for
                                    the entire order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Lockid"
                            type="yfctype:Lockid" use="optional">
                            <xsd:annotation yfc:columnName="LOCKID" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>An integer used to track and prevent concurrent
                                    modifications.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="MaxOrderStatus"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="MaxOrderStatusDesc"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="MinOrderStatus"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="MinOrderStatusDesc"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="Modifyprogid"
                            type="yfctype:ProgramID" use="optional">
                            <xsd:annotation
                                yfc:columnName="MODIFYPROGID" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>Program that modified the record.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Modifyts"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation yfc:columnName="MODIFYTS"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>Timestamp when the record was last modified.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Modifyuserid"
                            type="yfctype:Modifyuserid" use="optional">
                            <xsd:annotation
                                yfc:columnName="MODIFYUSERID" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>User who modified the record.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="NextAlertTs"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation
                                yfc:columnName="NEXT_ALERT_TS" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This field is computed, and used internally by
                                    the order monitor to identify when the order is next eligible
                                    for monitoring for a business exception.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="NoOfAuthStrikes" use="optional">
                            <xsd:annotation
                                yfc:columnName="NO_OF_AUTH_STRIKES" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>No. of credit card authorization strikes failed
                                    for the order.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                        <xsd:restriction base="yfctype:Digit" />
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                        <xsd:restriction base="xsd:string">
                                            <xsd:enumeration value="" />
                                        </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="NotifyAfterShipmentFlag"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="NOTIFY_AFTER_SHIPMENT_FLAG"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This field is not currently used.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrderComplete"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="ORDER_CLOSED" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This flag indicates whether the order has reached
                                    the end of its processing lifecycle.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrderDate"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation yfc:columnName="ORDER_DATE"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The date on which the order was taken. If not
                                    passed during order creation, it is defaulted to current system
                                    date.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrderHeaderKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The primary key for the YFS_ORDER_HEADER table.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrderName"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation yfc:columnName="ORDER_NAME"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The name given to the order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrderNo"
                            type="yfctype:VarcharDocumentNo" use="optional">
                            <xsd:annotation yfc:columnName="ORDER_NO"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The number of the order, which can be passed
                                    during order creation or generated based on a system rule
                                    setting.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrderPurpose"
                            type="yfctype:EnumOrderPurpose" use="optional">
                            <xsd:annotation
                                yfc:columnName="ORDER_PURPOSE" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This indicates what, if any, specific purpose
                                    this order was created for. The only valid value is 'EXCHANGE'
                                    which means the order is created for exchange purpose. But, the
                                    system may create an order with OrderPurpose of REFUND for
                                    internal use.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrderType"
                            type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="ORDER_TYPE"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The order classification attribute. This field
                                    can be used for reporting purposes or to build conditions for
                                    modeling your business process. Sterling Selling And Fulfillment
                                    Suite system has no default logic based on this field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OriginalTax"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation
                                yfc:columnName="ORIGINAL_TAX" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>Total tax amount at the time of the order
                                    creation.This attribute only reflects the header tax.This field
                                    is not updated later in the cycle and can be used to track
                                    changes made since the order creation.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OriginalTotalAmount"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation
                                yfc:columnName="ORIGINAL_TOTAL_AMOUNT"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The total amount at the time of the order
                                    creation. This field is not updated later in the cycle and can
                                    be used to track changes made since the order creation.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OtherCharges"
                            type="yfctype:ComputedMoney" use="optional">
                            <xsd:annotation
                                yfc:columnName="OTHER_CHARGES" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The sum of the other charges at the header level.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Override"
                            type="yfctype:Flag" use="optional" />
                        <xsd:attribute name="PaymentStatus"
                            type="yfctype:VarcharShortStatus" use="optional">
                            <xsd:annotation
                                yfc:columnName="PAYMENT_STATUS" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The information about the payment status of the
                                    order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PendingTransferIn"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation
                                yfc:columnName="PENDING_TRANSFER_IN"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The amount of funds available for the exchange
                                    order. This is the same as the total order amount of the return
                                    order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PersonalizeCode"
                            type="yfctype:Text-20" use="optional">
                            <xsd:annotation
                                yfc:columnName="PERSONALIZE_CODE" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The code that describes the personalization
                                    required for the order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PriceOrder"
                            type="yfctype:Flag" use="optional" />
                        <xsd:attribute name="PriceProgramName"
                            type="yfctype:Text-40" use="optional" />
                        <xsd:attribute name="PriorityCode"
                            type="yfctype:Text-20" use="optional">
                            <xsd:annotation
                                yfc:columnName="PRIORITY_CODE" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The priority code of the order. External logic
                                    can be built for prioritizing orders.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PriorityNumber"
                            type="yfctype:Priority" use="optional">
                            <xsd:annotation
                                yfc:columnName="PRIORITY_NUMBER" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The numeric priority code as opposed to
                                    alphanumeric field PRIORITY_CODE.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PropagateCancellations"
                            type="yfctype:Flag" use="optional" />
                        <xsd:attribute name="Purpose"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation yfc:columnName="PURPOSE"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This field is passed out in the tax calculation
                                    user exits.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ReqDeliveryDate" use="optional">
                            <xsd:annotation
                                yfc:columnName="REQ_DELIVERY_DATE" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The requested delivery date. This date is stamped
                                    on order lines (as requested delivery date) during order
                                    creation if no requested delivery date was passed at the order
                                    line level.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                        <xsd:restriction base="yfctype:TimeStamp" />
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                        <xsd:restriction base="xsd:string">
                                            <xsd:enumeration value="" />
                                        </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="ReserveInventoryFlag"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>This flag indicates whether inventory is reserved
                                    for this order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ReturnByGiftRecipient"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="RETURN_BY_GIFT_RECIPIENT"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This flag indicates whether the return was
                                    initiated by the gift recipient. This field is only used by
                                    return orders.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute
                            name="ReturnOrderHeaderKeyForExchange"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="RETURN_OH_KEY_FOR_EXCHANGE"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The return order header key for which this
                                    exchange order was created.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SCAC"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation yfc:columnName="SCAC" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The carrier assigned to the order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SaleVoided"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation yfc:columnName="SALE_VOIDED"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This flag indicates whether the sale transaction
                                    has been voided.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ScacAndService"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="ScacAndServiceKey"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="SearchCriteria1"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="SEARCH_CRITERIA_1" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This field is provided for allowing custom
                                    searches. You can decide at implementation time what should be
                                    stored in this field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SearchCriteria2"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="SEARCH_CRITERIA_2" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This field is provided for allowing custom
                                    searches. You can decide at implementation time what should be
                                    stored in this field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SellerOrganizationCode"
                            type="yfctype:OrgCode" use="optional">
                            <xsd:annotation
                                yfc:columnName="SELLER_ORGANIZATION_CODE"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The seller organization that is associated with
                                    this order. This must be a valid organization defined in the
                                    system.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipToID"
                            type="yfctype:ID-40" use="optional">
                            <xsd:annotation yfc:columnName="SHIP_TO_ID"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The identifier of the ship to customer. If a
                                    customer definition representing the buyer organization exists
                                    within the system, the ShipToID can represent the CustomerID of
                                    the customer. Otherwise, the ShipToID can represent the PersonID
                                    of the ship to address or the receiving node of the order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipToKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation yfc:columnName="SHIP_TO_KEY"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The ship to address that is associated with this
                                    order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SourceIPAddress"
                            type="yfctype:Text-20" use="optional">
                            <xsd:annotation
                                yfc:columnName="SOURCE_IP_ADDRESS" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>IP address of the system from which the order was
                                    placed.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SourcingClassification"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation
                                yfc:columnName="SOURCING_CLASSIFICATION"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The sourcing classification that can be used
                                    during scheduling. This field can be matched with the
                                    corresponding field in the YFS_SOURCING_RULE_HDR table, and the
                                    appropriate sourcing rule can be selected.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Status" type="xsd:string" use="optional" />
                        <xsd:attribute name="TaxExemptFlag"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="TAX_EXEMPT_FLAG" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>This flag indicates whether the buyer is extempt
                                    from paying taxes.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TaxExemptionCertificate"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation
                                yfc:columnName="TAX_EXEMPTION_CERTIFICATE"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The buyer's tax exemption certificate.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TaxJurisdiction"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation
                                yfc:columnName="TAX_JURISDICTION" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The tax jurisdiction of the buyer.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TaxPayerId"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation yfc:columnName="TAXPAYER_ID"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The identifier of the buyer.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TermsCode"
                            type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="TERMS_CODE"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The terms code of the order. This is passed on to
                                    the invoice and published with the invoice.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TotalAdjustmentAmount"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation
                                yfc:columnName="TOTAL_ADJUSTMENT_AMOUNT"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>the sum of all the credit and debit memos issued
                                    against this order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="isHistory"
                            type="xsd:string" use="optional" />
                    </xsd:complexType>
                </xsd:element>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Shipments">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="1" ref="yfc:Shipment" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Shipment">
        <xsd:annotation yfc:tableName="YFS_SHIPMENT">
            <xsd:documentation>Stores shipment information. A shipment is a collection of order
                lines which will be shipped as a unit. One or more orders from a buyer organization
                may be grouped into the same shipment to minimize transportation costs.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" name="Extn">
                    <xsd:complexType>
                        <xsd:attribute name="DeliveredBy"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="DeliveredMode"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="ExtnChannel"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="ExtnDeliveryFailureReason"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="ExtnEnteredBy"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="ExtnInvoiceNo"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="ExtnIsLinkedRO"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="ExtnIsLinkedTradeIn"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="ExtnIsSTSFlag"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="ExtnIsValidSTS"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="ExtnSTStoreCode"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="ExtnTMSShipmentNo"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="ExtnTradeInShipmentNo"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="TPInvoiceRetryCount"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="TPInvoiceSent"
                            type="xsd:string" use="optional" />
                    </xsd:complexType>
                </xsd:element>
            </xsd:all>
            <xsd:attribute name="ActualFreightCharge"
                type="yfctype:Money" use="optional">
                <xsd:annotation yfc:columnName="ACTUAL_FREIGHT_CHARGE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The actual freight charge for transporting the shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ActualShipmentDate"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="ACTUAL_SHIPMENT_DATE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The actual shipment date. This is updated by the
                        confirmShipment API.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AllowOverage" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="ALLOW_OVERAGE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This flag indicates whether an overage is allowed for the
                        shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AssignedToUserId"
                type="yfctype:LoginId" use="optional">
                <xsd:annotation yfc:columnName="ASSIGNED_TO_USER_ID" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The backroom pick user who is currently working on this
                        shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AssociationAction"
                type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Association Action.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BBNMinVolume" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="BBN_MIN_VOLUME" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The normalized minimum volume of the Break Bulk Node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BBNMinWeight" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="BBN_MIN_WEIGHT" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The normalized minimum weight of the Break Bulk Node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BillToAddressKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="BILL_TO_ADDRESS_KEY" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The bill to address key that is associated with this
                        shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BillToCustomerId" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="BILL_TO_CUSTOMER_ID" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The bill to customer id that is associated with this
                        shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BreakBulkLoadKey" type="xsd:string" use="optional" />
            <xsd:attribute name="CODPayMethod" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="COD_PAY_METHOD" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The pay method for the COD amount.Valid values are CASH,
                        CASHIERS_CHECK, BUSINESS_CHECK, MONEY_ORDER, PERSONAL_CHECK, ANY. This will
                        be a common code, with code type COD_PAY_METHOD.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CarrierAccountNo"
                type="yfctype:AccountNo" use="optional">
                <xsd:annotation yfc:columnName="CUSTCARRIER_ACCOUNT_NO" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The account number of the customer or buying organization
                        with the carrier. This is used for arriving at the billing account number.
                        See YFS_SHIPMENT_CONTAINER.carrier_bill_account.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CarrierServiceCode"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CARRIER_SERVICE_CODE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The carrier service availed for transporting this shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CarrierType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CARRIER_TYPE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The type of carrier. This field is denormalized from
                        YFS_SCAC_AND_SERVICE for the scac and carrier service code on the shipment.
                        Typical values are PARCEL, LTL, and so forth.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Code" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CODE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CommercialValue" type="yfctype:Money" use="optional">
                <xsd:annotation yfc:columnName="COMMERCIAL_VALUE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The commercial or declared value of the shipment for
                        insurance purposes.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Currency" type="yfctype:Currency" use="optional">
                <xsd:annotation yfc:columnName="CURRENCY" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The currency corresponding to the shipment charges.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DeliveryCode"
                type="yfctype:DeliveryCode" use="optional">
                <xsd:annotation yfc:columnName="DELIVERY_CODE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The field is deprecated and is no longer in use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DeliveryMethod" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="DELIVERY_METHOD" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The delivery method for the Product Line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DeliveryTS" type="yfctype:Date" use="optional">
                <xsd:annotation yfc:columnName="DELIVERY_TS" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The date at which the shipment is delivered at its intended
                        destination.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DestinationZone" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="DESTINATION_ZONE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DoNotVerifyCaseContent"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="DO_NOT_VERIFY_CASE_CONTENT" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>Thif flag indicates whether the case content is to be
                        verified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DoNotVerifyPalletContent"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="DO_NOT_VERIFY_PALLET_CONTENT" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This flag indicates whether the pallet content is to be
                        verified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DocumentType" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="DOCUMENT_TYPE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>A classification of data on which a particular set of
                        business processes must be applied. In this case, it happens to be the
                        document type of the order document - basically the document type of the
                        orders comprising the shipment. A document type derived from the base
                        document type 0001 - ORDER.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DownLoadCount" type="yfctype:Count" use="optional">
                <xsd:annotation yfc:columnName="DOWNLOAD_COUNT" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The number of times that the shipment information has been
                        downloaded from the user interface.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EnterpriseCode" type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="ENTERPRISE_CODE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The enterprise to which this shipment belongs.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EspCheckRequired" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="ESP_CHECK_REQUIRED" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This flag indicates whether an ESP check is required. This is
                        a computed flag based on freight terms configuration.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ExpectedDeliveryDate"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="EXPECTED_DELIVERY_DATE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The date on which the shipment is expected to be delivered at
                        the buyer organization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ExpectedShipmentDate"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="EXPECTED_SHIPMENT_DATE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The shipment date as planned or arrived at in the system.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ExportTaxpayerID" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="EXPORT_TAXPAYER_ID" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The tax payer ID of the shipping organization if the shipment
                        is meant for export.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FreightTerms" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="FREIGHT_TERMS" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The entity that pays the freight. Freight Terms determines
                        whether a shipment is Pre Paid, 3rd Party Billing or Collect Billing.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FromAddressKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="FROM_ADDRESS_KEY" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The from address key that is associated with this shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FromAppointment" type="yfctype:Date" use="optional">
                <xsd:annotation yfc:columnName="FROM_APPOINTMENT" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The time after which an appointment can be taken by the ship
                        node with the carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HasOtherShipments" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="HAS_OTHER_SHIPMENTS" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HazardousMaterialFlag"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="HAZARDOUS_MATERIAL_FLAG"
                    yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This flag indicates whether the shipment carries an item that
                        is marked hazardous.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ITDate" type="yfctype:Date" use="optional">
                <xsd:annotation yfc:columnName="IT_DATE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The date on which the IT number was stamped.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsPackProcessComplete"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="IS_PACK_PROCESS_COMPLETE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This flag indicates whether the Pack Process is complete for
                        all containers in the shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsPackingRequired" type="xsd:string" use="optional" />
            <xsd:attribute name="IsPackingRequiredWithContents"
                type="xsd:string" use="optional" />
            <xsd:attribute name="IsProductPlacingComplete"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="IS_PRODUCT_PLACING_COMPLETE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This flag indicates whether placing products into container
                        is complete.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsShipmentLevelIntegration"
                type="xsd:string" use="optional" />
            <xsd:attribute name="IsSingleOrder" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_SINGLE_ORDER" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This flag indicates whether the shipment is for a single
                        order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LinesEntered" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="LINES_ENTERED" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This flag indicates whether the lines are entered for the
                        shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Lockid" type="yfctype:Count" use="optional">
                <xsd:annotation yfc:columnName="LOCKID" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>An integer used to track and prevent concurrent
                        modifications.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManuallyEntered" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="MANUALLY_ENTERED" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This flag indicates whether the shipment is manually entered.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NextAlertTs" type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="NEXT_ALERT_TS" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The timestamp for the next alert. This is a computed field,
                        which is used internally by the shipment monitor to proactively identify
                        when the shipment could potentially be realerted on a business exception as
                        per the monitor rules setup.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NumOfCartons"
                type="yfctype:ContainerCount" use="optional">
                <xsd:annotation yfc:columnName="NUM_OF_CARTONS" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The number of cartons in the shipment. This field is only an
                        approximation and not accurate depiction of the number of containers in a
                        shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NumOfPallets"
                type="yfctype:ContainerCount" use="optional">
                <xsd:annotation yfc:columnName="NUM_OF_PALLETS" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The number of pallets in the shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderAvailableOnSystem"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="ORDER_AVAILABLE_ON_SYSTEM" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This flag indicates whether the order is available on the
                        system.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OriginZone" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="ORIGIN_ZONE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OverrideManualShipmentEntry"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="OVERRIDE_MANUAL_SHIPMENT_ENTRY" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This flag indicates whether the "allow manual shipment entry"
                        flag in the receiving rule can be overridden.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PackAndHold" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="PACK_AND_HOLD" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This flag indicates whether the order needs to be packed and
                        put away for retrieval at a later date. It needs to be released manually in
                        order to ship the order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PackListType" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="PACKLIST_TYPE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The packlist type. One of the attributes used in shipment
                        consolidation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PickTicketPrinted" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="PICKTICKET_PRINTED" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This flag indicates whether the pick ticket has been printed.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PipelineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PIPELINE_KEY" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The pipeline to which this shipment belongs. This is
                        determined by the createShipment API based on the document type and several
                        other key attributes of the shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReleaseNo" type="yfctype:Count" use="optional">
                <xsd:annotation yfc:columnName="RELEASE_NO" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The release number of the OrderRelease for the shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RequestedCarrierServiceCode"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation
                    yfc:columnName="REQUESTED_CARRIER_SERVICE_CODE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The requested carrier service code.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RequestedDeliveryDate"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="REQUESTED_DELIVERY_DATE"
                    yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The date on which the shipment must be delivered at the buyer
                        organization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RequestedShipmentDate"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="REQUESTED_SHIPMENT_DATE"
                    yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The shipment date as requested by the buyer organization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SCAC" type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="SCAC" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The carrier by which this shipment will be transported.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ScacAndService" type="xsd:string" use="optional" />
            <xsd:attribute name="ScacAndServiceKey" type="xsd:string" use="optional" />
            <xsd:attribute name="ScacIntegrationRequired"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="SCAC_INTEGRATION_REQUIRED" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This flag indicates whether the carrier integration is
                        required for the shipment. This is a computed field, and is set to Y if
                        carrier integration is required for domestic or international shipment based
                        on the YFS_SCAC_EX setup for the seller organization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SellerOrganizationCode"
                type="yfctype:OrgCode" use="optional">
                <xsd:annotation
                    yfc:columnName="SELLER_ORGANIZATION_CODE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The seller organization to which the shipment belongs.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipDate" type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="SHIP_DATE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipMode" type="yfctype:ShipMode" use="optional">
                <xsd:annotation yfc:columnName="SHIP_MODE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>An attribute used to identify the mode of shipment. Typical
                        values are TL - Truck Load, LTL - Less Than Truck Load, etc. This attribute
                        can also be used to store any of your own categorizations.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipNode" type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="SHIPNODE_KEY" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The ship node where the shipment was created.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipToCustomerId" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="SHIP_TO_CUSTOMER_ID" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The ship to identifier. If a customer definition representing
                        the buyer organization exists within the system, the ShipToID can represent
                        the CustomerID of the customer. Otherwise, the ShipToID can represent the
                        PersonID of the ship to address or the receiving node of the order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipVia" type="yfctype:Text-10" use="optional">
                <xsd:annotation yfc:columnName="SHIP_VIA" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The means of shipment. This is usually Land, Air, Sea, or
                        Ocean.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipmentClosedFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="SHIPMENT_CLOSED_FLAG" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This flag indicates whether the shipment is closed. This is a
                        computed field, which is used internally as a way to logically mark data for
                        purge so that transactions do not look into this data. This data is later
                        purged by the purge agent at preset intervals. This is updated by the close
                        shipment agent.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipmentConfirmUpdatesDone"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="SHIPMENT_CONFIRM_UPDATES_DONE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This flag indicates whether the shipment confirm updates have
                        been done. This is an internal field, which is used by the confirmShipment
                        API to identify if shipment confirmation updates have already been performed
                        on the shipment. Since there are various ways through which shipment
                        confirmation updates could happen -- receiveIntransitUpdates, closeManifest,
                        or confirmShipment -- it is required that there be an attribute that
                        identifies if the shipment confirmation has indeed been done.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipmentContainerizedFlag"
                type="yfctype:EnumShipmentContainerizedFlag" use="optional">
                <xsd:annotation
                    yfc:columnName="SHIPMENT_CONTAINERIZED_FLAG" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This is an internal field that indicates the containerization
                        state of the shipment. The values being: 01 - not containerized, 02 -
                        containerization in progress and 03 - indicating complete containerization.
                        This is updated by all shipment APIs that containerize or uncontainerize.
                        APIs include createShipment, packShipment, unpackShipment, confirmShipment
                        and changeShipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipmentDeliverUpdatesDone"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="SHIPMENT_DELIVER_UPDATES_DONE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This internal field is used by the receiveIntransitUpdates
                        API to identify if delivery updates have already been done. Delivery updates
                        are possible by explicitly invoking the deliverShipment API.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipmentKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SHIPMENT_KEY" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The primary key for the YFS_SHIPMENT table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipmentNo" type="yfctype:DocumentNo" use="optional">
                <xsd:annotation yfc:columnName="SHIPMENT_NO" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The shipment number. This is a part of the unique key or
                        business key.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipmentPlannedFlag"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="SHIPMENT_PLANNED_FLAG" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This flag indicates whether the shipment is planned. This is
                        a computed field. When every leg of the shipment from source to destination
                        is included into loads, the shipment is said to be planned. This is updated
                        by the manageLoad API.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipmentSortLocationId"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation
                    yfc:columnName="SHIPMENT_SORT_LOCATION_ID" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The location identifier where sorting is done, based upon
                        shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipmentType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SHIPMENT_TYPE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The user-defined shipment type. This is used to indicate the
                        composition of the shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Status" type="yfctype:ShortStatus" use="optional">
                <xsd:annotation yfc:columnName="STATUS" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The status of the shipment. As various transactions in the
                        shipment's pipeline act upon the shipment, the shipment status changes.
                        Typical values are 1100 - Created, 1200 - Being Picked, 1400 - Shipped.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StatusDate" type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="STATUS_DATE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The date and time when the last status change took place.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ToAppointment" type="yfctype:Date" use="optional">
                <xsd:annotation yfc:columnName="TO_APPOINTMENT" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The time before which an appointment could be taken by the
                        ship node with the carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TotalActualCharge" type="yfctype:Money" use="optional">
                <xsd:annotation yfc:columnName="TOTAL_ACTUAL_CHARGE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The actual charge for transporting the shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TotalEstimatedCharge"
                type="yfctype:Money" use="optional">
                <xsd:annotation yfc:columnName="TOTAL_ESTIMATED_CHARGE" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The estimated charge for transporting the shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TotalNumOfPickableSKUs"
                type="xsd:string" use="optional" />
            <xsd:attribute name="TotalVolume" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="TOTAL_VOLUME" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The total volume of the shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TotalVolumeUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="TOTAL_VOLUME_UOM" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The unit of measure for the units of volume specified in
                        TotalVolume attribute of this shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TotalWeight" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="TOTAL_WEIGHT" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The total weight of the shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TotalWeightUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="TOTAL_WEIGHT_UOM" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The unit of measure for the units of weight specified in
                        Total_Weight attribute of this shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TrackingNo"
                type="yfctype:VarcharDocumentNo" use="optional">
                <xsd:annotation yfc:columnName="TRACKING_NO" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLines">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="1" name="OrderLine">
                    <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
                        <xsd:documentation>This table stores order line information, including item
                            details and shipping information. Multiple lines can be created for a
                            single order defined in the YFS_ORDER_HEADER table.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element maxOccurs="1" minOccurs="0" name="Instructions">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element
                                            maxOccurs="unbounded"
                                            minOccurs="0" ref="yfc:Instruction" />
                                    </xsd:sequence>
                                    <xsd:attribute
                                        name="NumberOfInstructions"
                                        type="xsd:string" use="optional" />
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element maxOccurs="1" minOccurs="1" name="Item">
                                <xsd:annotation yfc:tableName="YFS_ITEM">
                                    <xsd:documentation>The table stores product information.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute
                                        name="BundleFulfillmentMode"
                                        type="yfctype:Text-2" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="BUNDLE_FULFILLMENT_MODE"
                                            yfc:tableName="YFS_ITEM">
                                            <xsd:documentation>The way in which the bundle will be
                                                fulfilled. It can be Ship Together(01), Ship
                                                Independent(00), or Deliver Together(02) for Bundle
                                                Item. Default value is Ship Together(01) for Bundle
                                                Item.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="CostCurrency"
                                        type="yfctype:Currency" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="COST_CURRENCY" yfc:tableName="YFS_ITEM">
                                            <xsd:documentation>The currency in which the item's cost
                                                is defined.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                        name="CountryOfOrigin"
                                        type="yfctype:Country" use="optional">
                                        <xsd:annotation
                                            yfc:columnName="COUNTRY_OF_ORIGIN"
