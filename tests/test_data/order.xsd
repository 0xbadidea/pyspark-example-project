                                        type="xsd:string" use="optional" />
                                    <xsd:attribute
                                        name="ExtnOmsExternalCreatets"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute
                                        name="ExtnOmsExternalModifyts"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute
                                        name="ExtnPOSPaymentReconNo"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute
                                        name="ExtnPublishToSAP"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute name="IssuerName"
                                        type="xsd:string" use="optional" />
                                    <xsd:attribute
                                        name="TenderDescription"
                                        type="xsd:string" use="optional" />
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                        <xsd:attribute name="Action" type="xsd:string" use="optional" />
                        <xsd:attribute
                            name="AwaitingAuthInterfaceAmount"
                            type="xsd:string" use="optional" />
                        <xsd:attribute
                            name="AwaitingChargeInterfaceAmount"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="BillToKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation yfc:columnName="BILL_TO_KEY"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The bill to address that is associated with this
                                    order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ChargeSequence"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="CheckNo" type="xsd:string" use="optional" />
                        <xsd:attribute name="CheckReference"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="Createprogid"
                            type="yfctype:ProgramID" use="optional">
                            <xsd:annotation
                                yfc:columnName="CREATEPROGID" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>Program that created the record.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Createts"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation yfc:columnName="CREATETS"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>Timestamp when the record was created.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Createuserid"
                            type="yfctype:Createuserid" use="optional">
                            <xsd:annotation
                                yfc:columnName="CREATEUSERID" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>User who created the record.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CreditCardExpDate"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="CreditCardName"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="CreditCardType"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="CustomerPONo"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="CUSTOMER_PO_NO" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The PO number of the customer. For purchase
                                    orders, this can be used for storing the purchase order number
                                    of the purchasing system if that number is not already stored as
                                    part of the order number.</xsd:documentation>
                            </xsd:annotation>
