                        <xsd:attribute name="DisplayCreditCardNo"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="DisplayCustomerAccountNo"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="DisplayPaymentReference1"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="DisplayPrimaryAccountNo"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="DisplaySvcNo"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="ExtendedFlag"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="IncompletePaymentType"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="Lockid"
                            type="yfctype:Lockid" use="optional">
                            <xsd:annotation yfc:columnName="LOCKID" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>An integer used to track and prevent concurrent
                                    modifications.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="MaxChargeLimit"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="Modifyprogid"
                            type="yfctype:ProgramID" use="optional">
                            <xsd:annotation
                                yfc:columnName="MODIFYPROGID" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>Program that modified the record.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Modifyts"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation yfc:columnName="MODIFYTS"
                                yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>Timestamp when the record was last modified.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Modifyuserid"
                            type="yfctype:Modifyuserid" use="optional">
                            <xsd:annotation
                                yfc:columnName="MODIFYUSERID" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>User who modified the record.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrderHeaderKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_ORDER_HEADER">
                                <xsd:documentation>The primary key for the YFS_ORDER_HEADER table.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PayMethodOverride"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="PaymentKey"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="PaymentReference1"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="PaymentReference2"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="PaymentReference3"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="PaymentReference4"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="PaymentReference5"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="PaymentReference6"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="PaymentReference7"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="PaymentReference8"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="PaymentReference9"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="PaymentType"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="PaymentTypeGroup"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="PlannedRefundAmount"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="PrimaryAccountNo"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="RequestedAuthAmount"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="RequestedChargeAmount"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="RequestedRefundAmount"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="SecondaryAmount"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="SuspendAnyMoreCharges"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="TotalAltRefundedAmount"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="TotalAuthorized"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="TotalCharged"
                            type="xsd:string" use="optional" />
                        <xsd:attribute name="TotalRefundedAmount"
                            type="xsd:string" use="optional" />
